library(lubridate)
get_wateryear<-function(date,yearstart){
yearstart=10
x<-as.character(ifelse(month(date)<yearstart,year(date), year(date)+1))
x<-paste0((as.numeric(str_sub(x,1))-1),"-",str_sub(x, 1))
